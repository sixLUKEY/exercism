#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/sixlukey/Development/Exercism/cpp/hello-world/build
  /Users/sixlukey/Development/Exercism/cpp/hello-world/build/Debug/hello-world
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/sixlukey/Development/Exercism/cpp/hello-world/build
  /Users/sixlukey/Development/Exercism/cpp/hello-world/build/Release/hello-world
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/sixlukey/Development/Exercism/cpp/hello-world/build
  /Users/sixlukey/Development/Exercism/cpp/hello-world/build/MinSizeRel/hello-world
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/sixlukey/Development/Exercism/cpp/hello-world/build
  /Users/sixlukey/Development/Exercism/cpp/hello-world/build/RelWithDebInfo/hello-world
fi

